jQuery(document).ready(function(){jQuery("img").each(function(){var e,n,s,o,i,a,t=jQuery(this).attr("src");typeof t!==typeof void 0&&t!==!1&&(e=jQuery(this).attr("alt"),s=t,n=s.split("/"),o=n[n.length-1],i=o.split("."),a=i[0],(e==""||typeof e===typeof void 0||e===!1)&&jQuery(this).attr("alt",a))})})