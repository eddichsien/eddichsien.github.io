jQuery(document).ready(function(){let e=!1;function t(){jQuery(window).width()<769?(e||jQuery(".menu_container").on("click",function(e){if(e.preventDefault(),jQuery(this).find(".menu_content_container").hasClass("active"))console.log("has active"),jQuery(this).find(".accordion_content").slideUp(500),jQuery(".menu_content_container").removeClass("active");else{console.log("no class active"),jQuery(".menu_content_container").removeClass("active"),jQuery(this).find(".menu_content_container").addClass("active");var t=jQuery(this).find(".accordion_content");t.slideDown(200),jQuery(".accordion_content").not(t).slideUp("fast")}}),e=!0):e&&(e=!1)}t(),jQuery(window).on("resize",function(){t()})})