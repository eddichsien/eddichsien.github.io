jQuery(document).ready(function(){jQuery("a[href*='"+window.location.href+"']").addClass("active-link"),jQuery("body").children().each(function(){jQuery(this).html(jQuery(this).html().replace(/&#8232;/g," "))});let i=0,r=jQuery(window),h=r.height(),s=jQuery(".desktop_nav_sticky"),o=jQuery(".mobile_top_nav"),t=jQuery(".mobile_top_nav_sticky"),a=jQuery(".mobile_nav .menu-item-has-children ul").hide(),e=jQuery(".mobile_bottom_nav"),n=jQuery(".mobile_inner_container").hide(),c=jQuery(".hamburger_menu"),m=jQuery(".menu_wrapper"),f=jQuery(".menu-item-has-children > a"),l=jQuery(".mobile_nav .dh_ft_minus_icon"),d=jQuery(".mobile_nav .dh_ft_plus_icon"),p=jQuery(".mobile_inner_container a"),u=jQuery(".menu_tab_container");r.scroll(function(){let n=jQuery(this).scrollTop();n<i&&n>h?(s.addClass("visible_nav").removeClass("hidden_nav"),t.addClass("visible_nav").removeClass("hidden_nav"),u.addClass("stickyNavVisible")):(s.hasClass("visible_nav")&&s.addClass("hidden_nav").removeClass("visible_nav"),e.hasClass("open")||(t.addClass("hidden_nav").removeClass("visible_nav-nav"),u.removeClass("stickyNavVisible"))),i=n}),function(){m.on("click",function(){c.toggleClass("animate"),jQuery(document.body).toggleClass("stop_scrolling"),e.hasClass("open")?(a.hide(),jQuery(".mobile_nav .menu-item-has-children").removeClass("active"),e.removeClass("open"),o.removeClass("open"),t.removeClass("open"),n.fadeToggle("fast",function(){e.slideToggle(300)}),jQuery(".mobile_bottom_nav ul li").css("animation-name","none"),jQuery(".mobile_bottom_nav .mobile_inner_container a.social").css("animation-name","none")):(e.addClass("open"),o.addClass("open"),t.addClass("open"),e.slideToggle(300,function(){n.fadeToggle("fast")}),d.removeClass("hide_icon"),l.addClass("hide_icon"),jQuery(".mobile_bottom_nav ul li").each(function(e){var t=.1+e*.03;jQuery(this).css("animation-name","slideInLeft"),jQuery(this).css("animation-delay",t+"s")}))})}(),function(){a.removeClass("active"),d.removeClass("hide_icon"),l.addClass("hide_icon"),f.click(function(e){e.preventDefault();let t=jQuery(this),n=t.closest("ul"),s=n.find(".active"),o=t.closest("li"),i=o.hasClass("active"),a=0;n.find("ul").slideUp(function(){++a==n.find("ul").length&&(s.removeClass("active"),s.find(".dh_ft_plus_icon").removeClass("hide_icon"),s.find(".dh_ft_minus_icon").addClass("hide_icon"))}),i||(o.children("ul").slideDown(),o.addClass("active"),t.find(".dh_ft_plus_icon").addClass("hide_icon"),t.find(".dh_ft_minus_icon").removeClass("hide_icon"))}),jQuery(".mobile_nav .sub_menu a").click(function(){c.toggleClass("animate"),jQuery(document.body).toggleClass("stop_scrolling"),e.addClass("open"),e.slideToggle(300,function(){n.fadeToggle("fast")})})}()})