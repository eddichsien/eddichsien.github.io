jQuery(document).ready(function(){let t=jQuery(".menu_tab_trigger"),o=jQuery(".menu_content_container").hide(),i=jQuery(".download_menu").hide();jQuery(".menu_content_container:first-of-type").show(),jQuery("#food_menu").show(),jQuery(".download_menu:first-of-type").show();function n(){let e=jQuery(".js_bottomless_lunch_menu_slider");e.hasClass("slick-initialized")&&e.slick("unslick"),e.slick({slidesToShow:1,slidesToScroll:1,infinite:!0,arrows:!1,dots:!0,autoplay:!0,autoplaySpeed:15e3})}n(),t.click(function(){t.removeClass("active"),o.hide(),i.hide(),jQuery(".menu_legend").hide(),jQuery(this).addClass("active"),currTab=jQuery(this).attr("data-content"),jQuery(".menu_content_container#"+currTab+"").fadeIn(),jQuery(".download_menu."+currTab+"").fadeIn(),console.log(currTab),currTab=="food_menu"&&jQuery(".menu_legend").fadeIn(),n()});const a=new URLSearchParams(window.location.search),e=a.get("menu");if(e){jQuery(".menu_tab_trigger").removeClass("active"),jQuery(".download_menu").hide();const t=jQuery(`.menu_tab_trigger[data-content="${e}"]`);t.length?(t.addClass("active"),jQuery(".menuParam").show(),t.click()):console.warn(`No matching tab found for menu parameter: ${e}`)}let s=jQuery(".menu_slider");s.slick({slidesToShow:1.4,slidesToScroll:1,autoplay:!0,autoplaySpeed:3e3,infinite:!0,touchThreshold:20,variableWidth:!1,rows:0,arrows:!1,dots:!1}),s.slick("setPosition",0)})